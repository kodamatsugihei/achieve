<div class="container">
  <h2>ユーザー</h2>
  <div class="row">
    <div class="col-md-12">
      <% @users.each do |user| %>
          <div class="col-md-4">
            <%= profile_img(user) %>
            <%= link_to user.email, user_path(user.id) %>
            <%= render 'follow_form', user: user %>
          </div>
        <% end %>
    </div>
  </div>
</div>
